<% include ../h %>


    <div class="card">
      <div class="card-body">


        <form action="#" method="post" style="margin-bottom:50px">
            <div class="container">
                <div class="row">

                  <div class="col-sm">
                    <div class="form-group">
                      <label for="book_name">Book Name</label>
                      <input type="text" class="form-control"
                             id="book_name" name="book_name"
                             placeholder="Book Name">
                    </div>
                  </div>

                  <div class="col-sm">
                    <div class="form-group">
                      <label for="email">Author</label>
                      <input type="text" class="form-control"
                             id="author" name="author"
                             placeholder="Author">
                    </div>
                  </div>

                </div>


                <div class="row">

                  <div class="col-sm">
                    <div class="form-group">
                      <label for="major">Field</label>
                      <select class="form-control" id="field" name="field">
                          <option value="Electrical Engineering">Electrical Engineering</option>
                          <option value="Computer Science">Computer Science</option>
                          <option value="Pshycology">Pshycology</option>
                          <option value="Economics">Economics</option>
                      </select>
                    </div>
                  </div>

                  <div class="col-sm">
                    <div class="form-group">
                        <label for="major"> </label>
                        <a href="#" id="btn_submit" class="btn btn-primary"
                           style="width:100%;margin-top:+10px">Submit</a>
                    </div>
                  </div>

                </div>
            </div>

        </form>



        <div class="container">
            <div class="row">
                <div class="col-sm">
                </div>
                <div class="col-sm">
                  <div class="form-group">
                    <label for="search">Search</label>
                    <input type="text" class="form-control"
                           id="search" name="search"
                           placeholder="Search">
                  </div>
                </div>
            </div>

            <div class="row">
              <div class="col-sm">
                <table id="table_test"
                    class="table table-striped table-sm">
                    <thead>
                        <tr>
                            <th>Books</th>
                            <th>Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
              </div>
            </div>
        </div>




      </div>
    </div>





    <script type='text/javascript' src="/javascripts/main/test.js"></script>






